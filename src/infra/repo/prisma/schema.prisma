generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model userAccount {
  id       String @unique
  name     String
  email    String @unique
  password String
  picture  String
  roles    Roles  @default(user)
}

enum Roles {
  admin
  user
}
